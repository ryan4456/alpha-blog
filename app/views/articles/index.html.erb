<h1>Article List</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each { |item| %>
      <tr>
        <td><%= item.title %></td>
        <td><%= item.description %></td>
        <td>Placeholder</td>
      </tr>
    <% } %>
  </tbody>
</table>